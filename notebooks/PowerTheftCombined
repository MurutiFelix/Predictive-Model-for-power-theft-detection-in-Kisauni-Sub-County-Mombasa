ning\n","\n","pos_weight = (len(y_train) - sum(y_train)) / sum(y_train)\n","print(\"XGBoost scale_pos_weight:\", round(pos_weight, 2))\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"eEn2XCFzUvBt","executionInfo":{"status":"ok","timestamp":1751121252967,"user_tz":-180,"elapsed":20,"user":{"displayName":"NGARI FELIX MURUTI","userId":"12523811355684898077"}},"outputId":"8a19f36e-a403-4f2b-cd24-02ac3398d6f1"},"execution_count":8,"outputs":[{"output_type":"stream","name":"stdout","text":["XGBoost scale_pos_weight: 0.17\n"]}]},{"cell_type":"code","source":["#@title Jaya Optimization for XGBoost\n","# Applied Jaya algorithm to optimize XGBoost hyperparameters for binary classification\n","\n","from sklearn.metrics import f1_score\n","from xgboost import XGBClassifier\n","import numpy as np\n","\n","# Define fitness function (maximize F1-score macro and weighted average)\n","def evaluate_xgb(params):\n","    learning_rate, max_depth, n_estimators = params\n","    pos_weight = (len(y_train) - sum(y_train)) / sum(y_train)\n","\n","\n","    # Convert to proper types\n","    max_depth = int(max_depth)\n","    n_estimators = int(n_estimators)\n","\n","    # Create XGBoost model with scale_pos_weight and current params\n","    model = XGBClassifier(\n","        learning_rate=learning_rate,\n","        max_depth=max_depth,\n","        n_estimators=n_estimators,\n","        scale_pos_weight=pos_weight,\n","        eval_metric=\"aucpr\",\n","        use_label_encoder=False,\n","        random_state=42\n","    )\n","\n","    # Train and predict\n","    model.fit(X_train, y_train)\n","    preds = model.predict(X_test)\n","\n","    # Calculate combined F1 score\n","    f1_macro = f1_score(y_test, preds, average='macro')\n","    f1_weighted = f1_score(y_test, preds, average='weighted')\n","\n","    # Return average as fitness\n","    return (f1_macro + f1_weighted) / 2\n","\n","# Jaya algorithm\n","def jaya_optimize(fitness_func, bounds, num_candidates=10, num_generations=10):\n","    dim = len(bounds)\n","    candidates = np.random.rand(num_candidates, dim)\n","\n","    # Scale to bounds\n","    for i in range(dim):\n","        candidates[:, i] = candidates[:, i] * (bounds[i][1] - bounds[i][0]) + bounds[i][0]\n","\n","    for gen in range(num_generations):\n","        scores = np.array([fitness_func(ind) for ind in candidates])\n","        best = candidates[np.argmax(scores)]\n","        worst = candidates[np.argmin(scores)]\n","\n","        # Update candidates\n","        for i in range(num_candidates):\n","            new_candidate = candidates[i] + np.random.rand() * (best - abs(candidates[i])) \\\n","                                             - np.random.rand() * (worst - abs(candidates[i]))\n","            # Clamp to bounds\n","            for j in range(dim):\n","                new_candidate[j] = np.clip(new_candidate[j], bounds[j][0], bounds[j][1])\n","            candidates[i] = new_candidate\n","\n","        print(f\"Generation {gen+1} - Best Fitness: {max(scores):.4f}\")\n","\n","    best_index = np.argmax([fitness_func(ind) for ind in candidates])\n","    return candidates[best_index]\n","\n","# Define parameter bounds for XGBoost: [learning_rate, max_depth, n_estimators]\n","param_bounds = [\n","    (0.01, 0.3),     # learning_rate\n","    (3, 10),         # max_depth\n","    (50, 200)        # n_estimators\n","]\n","\n","# Run Jaya optimization\n","best_params = jaya_optimize(evaluate_xgb, param_bounds, num_candidates=10, num_generations=10)\n","\n","# Extract and cast final parameters\n","xgb_best_lr = round(float(best_params[0]), 3)\n","xgb_best_depth = int(round(best_params[1]))\n","xgb_best_estimators = int(round(best_params[2]))\n","\n","print(\"\\nOptimized XGBoost Parameters:\")\n","print(\"Learning Rate:\", xgb_best_lr)\n","print(\"Max Depth:\", xgb_best_depth)\n","print(\"N Estimators:\", xgb_best_estimators)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"F9SSO2TLRJH6","executio